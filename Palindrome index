int palindromeIndex(char* s) {
    int length =strlen(s);
    for(int i=0;i<length/2;i++){
        if(s[i]!=s[length-1-i]){
                if(s[i+1]==s[length-i-1]&&s[i+2]==s[length-i-2]){
                    return i;
                }
                if(s[i]==s[length-i-2]&&s[i+1]==s[length-i-3]){
                    return length-i-1;
                }
        }
    }
    return -1;
}
