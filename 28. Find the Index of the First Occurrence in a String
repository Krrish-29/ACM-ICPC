int strStr(string haystack, string needle) {
        int answer=-1,count=0;
        for(int i=0,j=0;i<haystack.length();i){
            if(haystack[i]==needle[j]){
                count++;
                j++;
                i++;
            }
            else{
                if(count>0)
                    i=(i-count+1);
                else
                    i++;
                j=0;
                count=0;
            }
            if(count==needle.length()){
                answer=i-j;
                break;
            }
        }
        return answer;
    }
